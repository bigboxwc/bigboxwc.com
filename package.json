{
	"name": "bigbox",
	"title": "BigBox",
	"author": "Spencer Finnell",
	"description": "Easily manage and scale WooCommerce product catalogs.",
	"version": "1.9.2",
	"license": "GPL-2.0-or-later",
	"main": "webpack.config.js",
	"engines": {
		"node": ">=8.0.0",
		"npm": ">=6.0.0"
	},
	"devDependencies": {
		"@wordpress/babel-preset-default": "1.3.0",
		"autoprefixer": "^7.2.4",
		"axios": "^0.17.1",
		"babel-core": "6.26.3",
		"babel-eslint": "8.2.5",
		"babel-loader": "7.1.4",
		"babel-plugin-import-glob": "^2.0.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"bootstrap": "^4.0.0",
		"choices.js": "^3.0.3",
		"cross-env": "^5.0.1",
		"eslint": "5.0.0",
		"eslint-config-wordpress": "2.0.0",
		"eslint-plugin-jsx-a11y": "6.0.3",
		"eslint-plugin-react": "7.10.0",
		"eslint-plugin-wordpress": "git://github.com/WordPress-Coding-Standards/eslint-plugin-wordpress.git#1774343f6226052a46b081e01db3fca8793cc9f1",
		"exports-loader": "^0.7.0",
		"extract-text-webpack-plugin": "4.0.0-beta.0",
		"file-loader": "^1.1.6",
		"jquery": "^3.3.1",
		"lodash": "^4.17.5",
		"node-sass": "^4.9.0",
		"popper.js": "^1.12.9",
		"postcss-loader": "^2.0.10",
		"prop-types": "^15.6.0",
		"raw-loader": "^0.5.1",
		"sass-loader": "^7.0.1",
		"stylelint": "^9.2.0",
		"stylelint-config-wordpress": "^13.0.0",
		"svg-sprite-loader": "^3.7.3",
		"svgo": "^1.0.5",
		"svgo-loader": "^2.1.0",
		"webpack": "^4.8.1",
		"webpack-cli": "^3.0.3"
	},
	"babel": {
		"presets": [
			"@wordpress/default",
			{
				"plugins": [
					"import-glob",
					"transform-react-jsx"
				]
			}
		]
	},
	"scripts": {
		"build": "cross-env BABEL_ENV=default NODE_ENV=production webpack",
		"dev": "cross-env BABEL_ENV=default webpack --watch",
		"lint": "./node_modules/.bin/eslint  .; exit 0",
		"lint:fix": "./node_modules/.bin/eslint . --fix; exit 0",
		"css-lint": "./node_modules/.bin/stylelint resources/**/*.scss --syntax scss; exit 0",
		"css-lint:fix": "./node_modules/.bin/stylelint resources/**/*.scss --syntax scss --fix; exit 0",
		"package-theme": "./bin/package-theme.sh",
		"setup": "npm install && composer install"
	}
}
